<!--
  Summary of processes governing the generation of artifacts.
-->
<bpcProcesses>
  <bpcProcess bpcID="P01" worksheetNumber="1">
    <title>Invoice with reference to a blanket or standard PO based on a contract</title>
    <doctypes>
      <doctype>Invoice</doctype>
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P02" worksheetNumber="1">
    <title>Invoice with reference to a blanket or standard PO without a contract</title>
    <doctypes>
      <doctype>Invoice</doctype>
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P03" worksheetNumber="1">
    <title>Invoice without reference to contract or purchase order</title>
    <doctypes>
      <doctype>Invoice</doctype>
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P04" worksheetNumber="1">
    <title>Credit Memo Pre-payment</title>
    <doctypes>
      <doctype>Credit Note</doctype>
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P05" worksheetNumber="1">
    <title>Credit Memo Post-payment</title>
    <doctypes>
      <doctype>Credit Note</doctype>
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P06" worksheetNumber="1">
    <title>Invoicing the delivery of goods and services against a prepayment</title>
    <doctypes>
      <doctype>Invoice</doctype>
      <!--<doctype>Credit Note</doctype> Temporarily elided for development-->
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P07" worksheetNumber="1">
    <title>Invoicing the delivery of goods and services against a payment authorization
at the time of order or purchase</title>
    <doctypes>
      <doctype>Invoice</doctype>
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P08" worksheetNumber="1">
    <title>Invoices with references to shipping documents(Advanced Shipping Notice,
Dispatch Advice, Receiving Advice)</title>
    <doctypes>
      <doctype>Invoice</doctype>
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P09" worksheetNumber="1">
    <title>Cancellation/corrective invoicing</title>
    <doctypes>
      <doctype>Invoice</doctype>
      <!--<doctype>Credit Note</doctype> Temporarily elided for development-->
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P10" worksheetNumber="1">
    <title>Partial and final invoicing</title>
    <doctypes>
      <doctype>Invoice</doctype>
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P11" worksheetNumber="1">
    <title>Self-billing</title>
    <doctypes>
      <doctype>Invoice</doctype>
      <!--<doctype>Purchase Order</doctype>-->
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P12" worksheetNumber="1">
    <title>Recurring billing (fixed amount)</title>
    <doctypes>
      <doctype>Invoice</doctype>
    </doctypes>
  </bpcProcess>
  <bpcProcess bpcID="P13" worksheetNumber="1">
    <title>Invoice with opening balance and usage (variable amount)</title>
    <doctypes>
      <doctype>Invoice</doctype>
    </doctypes>
  </bpcProcess>
</bpcProcesses>
