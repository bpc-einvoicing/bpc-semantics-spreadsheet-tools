<!--
    This stylesheet implements the BPC code list rules for version:

    v0.4 - test
    
    Business Payments Coalition Semantics Group - (skeleton:20200922-1340z)
-->

<!--
  Return true or false that the given value is found in the given list.
  
  If the value is an element, then the CCTS metadata is inspected for
  nuanced identification of the list.
  
  If the value is not an element, the value is cast as a string.
-->
   <xsl:function name="bpc:codelist" as="xsd:boolean">
  <!--the first argument is the optional singleton value to test-->
      <xsl:param name="item" as="item()?"/>
      <!--the second argument is the identifier of the code list-->
      <xsl:param name="lookupID" as="xsd:string"/>
      <!--the return indicates the value is found in the list; false when the
      value is absent or the listID is not recognized-->
      <xsl:variable name="element" as="element()?">
         <xsl:choose>
            <xsl:when test="$item instance of element()">
               <xsl:sequence select="$item"/>
            </xsl:when>
            <xsl:when test="$item instance of attribute()">
               <placebo>
                  <xsl:copy-of select="$item/../@*"/>
                  <xsl:value-of select="$item"/>
               </placebo>
            </xsl:when>
            <xsl:otherwise>
               <placebo>
                  <xsl:value-of select="$item"/>
               </placebo>
            </xsl:otherwise>
         </xsl:choose>
      </xsl:variable>
      <xsl:for-each select="$element">  
    <!--check that the list is recognized-->
         <xsl:choose>
            <xsl:when test="$lookupID='IANA-Media-Types'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-IANA-Media-Types)"/>
            </xsl:when>
            <xsl:when test="$lookupID='IANA-Media-Types-Including-Deprecated'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-IANA-Media-Types-Including-Deprecated)"/>
            </xsl:when>
            <xsl:when test="$lookupID='ISO-3166'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-ISO-3166)"/>
            </xsl:when>
            <xsl:when test="$lookupID='ISO-4217'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-ISO-4217)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-1001'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-1001)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-1373'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-1373)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-4451'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-4451)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-4461'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-4461)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-5153'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-5153)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-5189'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-5189)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-5305'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-5305)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-Rec-20'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-Rec-20)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-Rec-20-21'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-Rec-20-21)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-Rec-20-21-Including-Deleted'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-Rec-20-21-Including-Deleted)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-Rec-20-Including-Deleted'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-Rec-20-Including-Deleted)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-Rec-21'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-Rec-21)"/>
            </xsl:when>
            <xsl:when test="$lookupID='UNECE-Rec-21-Including-Deleted'">
               <xsl:sequence select="bpc:checkCode(.,$lookupID,$bpc:codes-UNECE-Rec-21-Including-Deleted)"/>
            </xsl:when>
            <xsl:otherwise>
               <xsl:message select="'Error: Missing a code list implementation for: ',                              $lookupID"/>
               <xsl:sequence select="false()"/>
            </xsl:otherwise>
         </xsl:choose>
      </xsl:for-each>
   </xsl:function>

